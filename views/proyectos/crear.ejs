<div class="card">
    <div class="card-header">
        Formulario de Captura
    </div>
    <div class="card-body">
        <h5 class="card-title">Datos del usuario</h5>
        <p class="card-text">

        <form method="post" action="/proyectos" enctype="multipart/form-data">

            <div class="form-group">
                <label for="my-input">Nombre</label>
                <input id="my-input" class="form-control" type="text" name="nombre">
             </div>

            <div class="form-group">
                <label for="my-input">Apellido</label>
                <input id="my-input" class="form-control" type="text" name="apellido">
            </div>


            <div class="form-group">
                <label for="my-input">Imagen</label>
                <input id="my-input" class="form-control-file" type="file" name="archivo">
            </div>
    
            <button class="btn btn-success" type="submit">Guardar</button>
            <a class="btn btn-primary" href="/proyectos">Cancelar</a>
        </form>


        </p>
    </div>
</div>



